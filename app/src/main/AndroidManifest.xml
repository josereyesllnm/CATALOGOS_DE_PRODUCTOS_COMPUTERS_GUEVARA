<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logoempresainicio"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.COMPUTERS_GUEVARA"
        tools:targetApi="31">
        <activity
            android:name=".COMPUTADORAS"
            android:exported="false"
            android:parentActivityName=".MainActivity2"/>
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.COMPUTERS_GUEVARA.">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivityescritorio"
            android:exported="false"
            android:parentActivityName=".ESCRITORIO" />
        <activity
            android:name=".ESCRITORIO"
            android:exported="false"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".LAPTOPS"
            android:exported="false"
            android:parentActivityName=".MainActivity2" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity2" />

        <activity
            android:name=".MainActivitylaptops"
            android:exported="false"
            android:parentActivityName=".LAPTOPS" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"></activity>
    </application>

</manifest>